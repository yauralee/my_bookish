<%= stylesheet_link_tag "register_login" %>

<div class="register">
  <h1 class="page_title">Welcome to Bookish</h1>
  <div class="col-md-4 col-md-offset-4 login_info">
    <%= form_for(:session, url: login_path) do |f| %>

        <%= f.label :邮箱 %>
        <%= f.email_field :email %>

        <%= f.label :密码 %>
        <%= f.password_field :password %>

        <%= f.submit "登录" %>
    <% end %>
    <span><%= link_to '我还没有账户,点击注册', signup_path %></span>
  </div>
</div>
